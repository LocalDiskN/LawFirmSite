@model IEnumerable<LawFirmSite.Models.TeamModel>
@{
    if (Request.IsAuthenticated && User.IsInRole("admin"))
    {
        Layout = "~/Views/Admin/Shared/_AdminLayout.cshtml";
        @Html.Partial("~/Views/Admin/Shared/_AdminTeamPartial.cshtml", Model)
    }
    else
    {
        @Html.Partial("_TeamPartial", Model)
    }

}