<script>
    var crumbstr = @Html.Raw(Json.Encode(ViewBag.LayoutModel.languages));

    for (var i = 0; i < crumbstr.length; i++) {
        if (crumbstr[i].Content !== "") {
            crumbstr = crumbstr[i];
            if (window.location.href.indexOf("Admin") === -1) {
                putCrumbs(document.getElementById("breadCrumbs"), [{ page: getValueFromDic(crumbstr.Content, "Home"), link: "/Home/" }, { page: getValueFromDic(crumbstr.Content, "Admin"), link: "/Admin/" }, { page: getValueFromDic(crumbstr.Content, "Create"), link: "#"}]);
            }
            break;
        }
    }
    var visSwitch = putSwitch(document.getElementById("visibilityswitch"), 25, "60px", "30px", true);

    document.getElementById("Create").onmousedown = function () {
        var model = {
            lang: crumbstr.Abbreviation, datetimeformat: getValueFromDic(crumbstr.Content, "DateFormat"), imgUrl: getRelativeImgUrl(document.getElementById("imlabdiv0-img").src), Explanation: document.getElementById("imlabdiv0-head").innerHTML, Explanation2: document.getElementById("imlabdiv0-label").innerHTML,
            TitleAbbr: TitleAbbrTA.value, Job: JobTA.value, Graduations: GraduationsTA.value, Salary: stof(SalaryTA.value), SGKPirim: stof(SGKPirimTA.value), Currency: CurrencyTA.value,
            StartingDate: document.getElementById("datepicker").value, LeaveDate: document.getElementById("datepicker2").value,
            PhoneNum: PhoneNumTA.value, PhoneNum2: PhoneNum2TA.value, Address: AddressTA.value, Address2: Address2TA.value,
            Email: EmailTA.value, Email2: Email2TA.value, isVisible: getSwitchState(visSwitch),
            Seri: SeriTA.value, Num: NumTA.value, SSNum: SSNumTA.value, Surname: SurnameTA.value, Name: NameTA.value,
            FathersName: FathersNameTA.value, MothersName: MothersNameTA.value, BirthPlace: BirthPlaceTA.value,
            TimeOfBirth: document.getElementById("datepicker3").value, MaritalStatus: MaritalStatusTA.value, Religion: ReligionTA.value,
            BloodType: BloodTypeTA.value, City: CityTA.value, District: DistrictTA.value, NeighborhoodOrVillage: NeighborhoodOrVillageTA.value,
            CiltNo: CiltNoTA.value, AileSiraNo: AileSiraNoTA.value, SiraNo: SiraNoTA.value, VerildigiYer: VerildigiYerTA.value,
            VerilisNedeni: VerilisNedeniTA.value, KayitNo: KayitNoTA.value, VerilisTarihi: document.getElementById("datepicker4").value
            
        };

        post("/Team/Create/", model, "post");
