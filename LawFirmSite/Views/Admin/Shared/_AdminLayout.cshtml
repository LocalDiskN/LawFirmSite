                    <li class="nav-item mr-3">
                        <a id="Home" class="LayoutlinkCss" style="" href="/Home/Index"></a>
                    </li>
                    <li class="nav-item mr-3">
                        <a id="About" class="LayoutlinkCss" style="" href="/About/Index"></a>
                    </li>
                    <li id="pmebaby" class="nav-item mr-3">
                        <a id="Practices" class="LayoutlinkCss" href="/Practices/Index"><img id="iimg" src="~/Images/DownIcon2.png" /></a>
                        <div id="practiceme" class="practicesDiv">

                        </div>
                    </li>
                    <li class="nav-item mr-3">
                        <a id="Team" class="LayoutlinkCss" href="/Team/Index"></a>
                    </li>
                    <li class="nav-item mr-3">
                        <a id="Articles" class="LayoutlinkCss" href="/Articles/Index"></a>
                    </li>
                    <li class="nav-item mr-3">
                        <a id="Announcements" class="LayoutlinkCss" href="/Announcements/Index"></a>
                    </li>
                    <li class="nav-item mr-5">
                        <a id="Contact" class="LayoutlinkCss" href="/Contact/Index"></a>
                    </li>
                    <li id="CPbuttonList" class="nav-item mr-3">
                        <a id="Admin" class="LayoutlinkCss" href="/Admin/Index"></a>
                        <div id="ControlPanel" class="practicesDiv">
                            <a id="Settings" class="LayoutlinkCss" href="/Account/Settings"></a>
                            <a id="Logout" class="LayoutlinkCss" href="/Account/Logout"></a>
                        </div>
                    </li>
                </ul>
            </div>
        </div>

