    <div class="body-content">
        @RenderBody()
        <hr />

    </div>


    <footer style="background-color:rgb(176, 75, 75); font: bold 16px arial;">
        <div id="footer" class="container">
            <div id="FSitemap">

            </div>

            <div id="FContactDiv">

            </div>

            <div id="fmap">

            </div>
        </div>
    </footer>
    <div id="copyrightdiv">

        <P>©Copyrigth 2018 ÇELEBİ HUKUK VE DANIŞMANLIK</P>
        <label>powered by Nazım Can Türkeş</label>
    </div>
    <script src="~/StyleNScript/Generals/PutLinksScript.js"></script>
    <script>
        var mouseoverflag = false;
        var mylist = document.getElementById("practiceme");
        var logout = document.getElementById("ControlPanel");
        var dropdis = document.getElementById("droplangs-child");

        var model = @Html.Raw(Json.Encode(ViewBag.LayoutModel));

        for (var i = 0; i < model.practices.length; i++) {
            var divLink = document.createElement("a");
            divLink.className = "linkCss";
            var hrme = document.createAttribute("href");
            hrme.value = "/Practices/Practice/?id=" + model.practices[i].idme;
            divLink.setAttributeNode(hrme);
            divLink.innerHTML = getInside(model.practices[i].Title);

            mylist.appendChild(divLink);
        }

        var fsitemp = document.getElementById("FSitemap");
        var contdiv = document.getElementById("FContactDiv");
        var fMap = document.getElementById("fmap");

        //window.location.href
        for (i = 1; i < model.languages.length; i++) {
            var divLink = document.createElement("a");
            divLink.className = "dropdownme-link";
            var hrme = document.createAttribute("href");
            var parpos = window.location.href.indexOf("?");
            if (parpos === -1) {
                hrme.value = window.location.href + "?lang=" + model.languages[i].Abbreviation;
            }
            else {
                parpos = window.location.href.indexOf("lang=");
                if (parpos === -1) {
                    hrme.value = window.location.href + "&lang=" + model.languages[i].Abbreviation;
                }
                else {
                    var parpos2 = window.location.href.indexOf("&", parpos + 5);
                    if (parpos2 === -1) {
                        hrme.value = window.location.href.substr(0, parpos + 5) + model.languages[i].Abbreviation;
                    }
                    else {
                        hrme.value = window.location.href.substr(0, parpos + 5) + model.languages[i].Abbreviation + window.location.href.substr(parpos2);
                    }
                }
            }
            divLink.setAttributeNode(hrme);
            divLink.innerHTML = getInside(model.languages[i].Title) + "(" + getInside(model.languages[i].Abbreviation) + ")";

            dropdis.appendChild(divLink);

            if (model.languages[i].Content !== "") {
                document.title = getValueFromDic(model.languages[i].Content, "WebsiteTitle") + " (Admin)";
                var linkarray = document.getElementsByClassName("LayoutlinkCss");

                for (var ai = 0; ai < linkarray.length; ai++) {
                    if (linkarray[ai].id === "Practices") {
                        linkarray[ai].innerHTML = getValueFromDic(model.languages[i].Content, linkarray[ai].id) + '<img id="iimg" src="/Images/DownIcon2.png" />';
                    }
                    else {
                        linkarray[ai].innerHTML = getValueFromDic(model.languages[i].Content, linkarray[ai].id);
                    }
                }

                var fpar = document.createElement("p");
                fpar.style.font = "26px Times New Roman";
                fpar.style.color = "white";
                fpar.style.marginBottom = "8%";
                fpar.innerHTML = getValueFromDic(model.languages[i].Content, "SiteMap");
                fsitemp.appendChild(fpar);

                for (var a = 1; a < linkarray.length - 3; a++) {
                    var divLink = document.createElement("a");
                    divLink.className = "FootCssLink";
                    var hrme = document.createAttribute("href");
                    hrme.value = "/" + linkarray[a].id + "/";
                    divLink.setAttributeNode(hrme);
                    divLink.innerHTML = getValueFromDic(model.languages[i].Content, linkarray[a].id);
                    fsitemp.appendChild(divLink);
                }
                var fpar = document.createElement("p");
                fpar.innerHTML = "<br />";
                fsitemp.appendChild(fpar);


                if (getControllerName() !== "Contact") {
                    var conHpar = document.createElement("p");
                    conHpar.color = "white";
                    conHpar.style.marginBottom = "5%";
                    conHpar.style.font = "26px Times New Roman";
                    conHpar.innerHTML = getValueFromDic(model.languages[i].Content, "ContactUs");
                    contdiv.appendChild(conHpar);
                    for (var ai = 1; ai < model.contacts.length; ai++) {
                        var conpar = document.createElement("p");
                        if (model.contacts[ai].Cicon === null) {
                            model.contacts[ai].Cicon = "";
                        }
                        var inner = model.contacts[ai].Cicon + "<br />";
                        for (var a = 0; a < model.contacts[ai].Contents.length; a++) {
                            inner = inner + model.contacts[ai].Contents[a] + "<br />";
                        }
                        conpar.innerHTML = inner;
                        contdiv.appendChild(conpar);
                    }
                    var inner = "";
                    for (var ai = (300 - parseInt(window.getComputedStyle(contdiv).getPropertyValue("height"))) / 18; ai > 1; ai--) {
                        inner = inner + "<br />";
                    }
                    var conpar = document.createElement("p");
                    conpar.innerHTML = inner;
                    contdiv.appendChild(conpar);
                }
                else {
                    contdiv.style.height = "0px";
                    fMap.style.height = "0px";
                }

            }
        }

        document.getElementById("Admin").addEventListener("mouseover", function () {

                animate(logout, 946, "opacity", "1", 300, true, "block");
            });
            document.getElementById("CPbuttonList").addEventListener("mouseover", function () {
                mouseoverflag = false;
            });

        document.getElementById("Practices").addEventListener("mouseover", function () {

            animate(mylist, 945, "opacity", "1", 300, true, "block");
        });

        document.getElementById("practiceme").addEventListener("mouseover", function () {
            mouseoverflag = false;
        });
        document.getElementById("iimg").addEventListener("mouseover", function () {
            mouseoverflag = false;
        });
        document.getElementById("iimg").addEventListener("mousedown", function () {
            mouseoverflag = false;
            animate(mylist, 945, "opacity", "1", 300, true, "block");
        });
        document.getElementById("iimg").addEventListener("mouseout", function () {
            mouseoverflag = false;
        });
        document.getElementById("Practices").addEventListener("mouseout", function () {
            mouseoverflag = true;
        });
        document.getElementById("pmebaby").addEventListener("mouseover", function () {
            mouseoverflag = false;
        });

        var isvisible = false;
        document.getElementById("navme").addEventListener("mouseover", function () {

            if (mouseoverflag) {
                mouseoverflag = false;
                animate(mylist, 945, "opacity", 0, 300, true, "none");

                animate(logout, 946, "opacity", 0, 300, true, "none");

                animate(dropdis, 45678, "opacity", 0, 100, true, "none");
            }
            else {
                mouseoverflag = true;
            }
        });
        document.getElementById("Language").addEventListener("mouseover", function () {
            animate(dropdis, 45678, "opacity", 1, 100, true, "block");
        });
        document.getElementById("Language").addEventListener("mousedown", function () {
            window.location.href = "/Admin/Translations/";
        });
        document.getElementById("down-main").addEventListener("mouseover", function () {
            mouseoverflag = false;
        });
        dropdis.addEventListener("mouseover", function () {
            mouseoverflag = false;
    });


        var footer = document.getElementById("footer");

        putlinkss(footer, 4587465, [{ inner: '<i class="fab fa-facebook-f"></i>', class: "socialbuttons facebookcolor", address: "#" }, { inner: '<i class="fab fa-twitter"></i>', class: "socialbuttons twittercolor", address: "#" }], "left", "10px", "10px");

        window.addEventListener("resize", function () {
            setHeighttoElems(footer, [fsitemp, contdiv, fMap], 60);
        });

        window.addEventListener("load", function () {
            setHeighttoElems(footer, [fsitemp, contdiv, fMap], 60);
        });

        if (document.getElementById("map") == undefined) {
            function initMap() {
                // The location of Uluru
                var uluruz = { lat: parseFloat(model.contacts[0].Contents[0]), lng: parseFloat(model.contacts[0].Contents[1]) };
                // The map, centered at Uluru
                var fmap = new google.maps.Map(
                    fMap, { zoom: 18, center: uluruz });
                // The marker, positioned at Uluru
                var marker = new google.maps.Marker({ position: uluruz, map: fmap });
            }
        }

    </script>

    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD6LP357YE5W9faJAnFbFLMbMBFwPV5TrQ&callback=initMap">
    </script>
</body>
</html>

