        <div id="down-main" style="position: absolute; right: 15%; top: 0px;">
            <label id="Language" class="LayoutlinkCss dropdownme-toggle"></label>
            <div id="droplangs-child" class="dropdownme-child">

            </div>
        </div>
        <div>

        </div>
        <div class="container">
            <a href="/Home/Index"><img src="~/Images/logo2-3.png" style="max-height: 80px;" class="navbar-brand" /></a>

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarTogglerDemo02">
